{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://localhost:3001/groupNew.json",
  "type": "object",
  "properties": {
    "users": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "username": {
            "type": "string"
          }
        },
        "additionalItems": false
      }
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    }
  },
  "additionalProperties": false,
  "required": [
    "users",
    "name"
  ]
}